[cells]
root = .
prelude = prelude
toolchains = toolchains
bootstrap = bootstrap
none = none

[cell_aliases]
config = prelude
ovr_config = prelude
fbcode = none
fbsource = none
fbcode_macros = none
buck = none

[external_cells]
prelude = bundled

[parser]
target_platform_detector_spec = target:root//...->bootstrap//platforms:default

[build]
execution_platforms = bootstrap//platforms:platforms

[project]
ignore = .git, target
